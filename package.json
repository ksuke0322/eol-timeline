    "prepare": "husky",
    "build": "react-router build && rm -f ./build/client/mockServiceWorker.js",
    "dev": "react-router dev",
    "start": "vite preview --outDir ./build/client --port 4173 --strictPort",
    "typecheck": "react-router typegen && tsc",
